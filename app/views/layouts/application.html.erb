<!DOCTYPE html>
<html>
  <head>
    <title>DunderMifflinPetShop</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <nav>
    <ol>
      <%= button_to "Dogs", dogs_path, method: :get %>
      <%= button_to "Employees", employees_path, method: :get %>
    <%= yield %>
  </body>
</html>
